{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "crlf",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "rangeStart": 0,
  "insertPragma": false,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "printWidth": 120,
        "tabWidth": 2
      }
    }
  ]
}
